{
  "name": "gulp-terser",
  "version": "1.1.6",
  "description": "gulp plugin, compressed es6+ code.",
  "scripts": {
    "start": "npm run clean && babel ./src --watch --out-dir ./lib",
    "build": "npm run clean && babel ./src --out-dir ./lib",
    "clean": "rimraf ./lib",
    "test": "mocha \"./test/build/**/*.js\"",
    "test-gulp": "gulp --gulpfile ./test/gulpfile.js",
    "get-version": "node ./bin/get-version.js"
  },
  "main": "lib/index.js",
  "license": "MIT",
  "keywords": [
    "gulp",
    "gulpplugin",
    "uglify-es",
    "terser"
  ],
  "author": {
    "name": "段昊辰",
    "email": "duanhaochen@126.com",
    "url": "https://github.com/duan602728596"
  },
  "dependencies": {
    "plugin-error": "^1.0.1",
    "terser": "^3.11.0",
    "through2": "^3.0.0",
    "vinyl-sourcemaps-apply": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "gulp": "^3.9.1",
    "gulp-plumber": "^1.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "yargs": "^12.0.5"
  }
